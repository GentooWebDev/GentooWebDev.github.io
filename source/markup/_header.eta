<header>
  <div class="logo">
    <img src="/assets/img/logo.png" alt="Gentoo Web Dev Logo">
  </div>

  <nav>
    <%~
      it.pages.map((page) => {
        const url = page === 'index' ? '/' : `/${page}`;
        return `<a href="${page == it.currentPage ? '#' : url}" class="${page == it.currentPage ? 'active' : ''}">${
          url == '/' ? 'Home' : page.charAt(0).toUpperCase() + page.slice(1)
        }</a>`;
      }).join('')
    %>
  </nav>
</header>
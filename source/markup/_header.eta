<header>
  <div class="logo">
    <img src="/assets/img/logo.png" alt="Gentoo Web Dev Logo">
  </div>

  <menu>
    <%~
      it.pages.map((page) => {
        const url = page === 'index' ? '/' : `/${page}`;
        return `<li><a href="${page == it.currentPage ? '#' : url}" class="${page == it.currentPage ? 'active' : ''}">${url}</a></li>`;
      }).join('')
    %>
    </menu>
  </div>
</header>